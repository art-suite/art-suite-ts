{
  "name": "@art-suite/art-core-ts",
  "version": "0.8.7",
  "description": "The full Art-Core-TS library",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/art-suite/art-suite-ts.git"
  },
  "license": "MIT",
  "author": "Shane Delamore",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npx sort-package-json;tsup src/index.ts --format esm,cjs --dts --sourcemap",
    "build:clean": "npm run clean && npm run build",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@art-suite/art-core-ts-communication-status": "^0.2.1",
    "@art-suite/art-core-ts-comprehensions": "^0.1.10",
    "@art-suite/art-core-ts-containers": "^0.4.2",
    "@art-suite/art-core-ts-json": "^0.3.1",
    "@art-suite/art-core-ts-string-case": "^0.1.6",
    "@art-suite/art-core-ts-string-lib": "^0.2.1",
    "@art-suite/art-core-ts-time": "^0.1.8",
    "@art-suite/art-core-ts-types": "^0.2.2"
  }
}
