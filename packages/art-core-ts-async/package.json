{
  "name": "@art-suite/art-core-ts-async",
  "version": "0.2.11",
  "description": "Async and Promise utilities for TypeScript",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/art-suite/art-suite-ts"
  },
  "license": "MIT",
  "author": "Shane Delamore",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npx sort-package-json;tsup src/index.ts --format esm,cjs --dts --sourcemap",
    "build:clean": "npm run clean && npm run build",
    "check:types": "tsc --noEmit",
    "check:types:fast": "tsc --noEmit --incremental",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@art-suite/art-core-ts-comprehensions": "^0.4.15",
    "@art-suite/art-core-ts-time": "^0.2.1"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.5"
  }
}
